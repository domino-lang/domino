composition MediumComposition {
    const n: Integer;

    instance rand = Rand {
        params {
            n:   n,
        }
    }

    instance fwd = Fwd {
        params {
            n:   n,
        }
    }

    compose {
        adversary: {
            UsefulOracle: fwd,
            AnotherUsefulOracle: UsefulOracle of rand,
        },
        fwd: {
            O: UsefulOracle of rand,
        },
    }
}
