package Fwd {
    params {
        n: Integer,
    }

    state {
        ctr: Integer,
    }

    import oracles {
        Oracle() -> Integer,
    }

    oracle Oracle() -> Integer  {
        y  <- invoke Oracle();
        return y;
    }

    oracle UselessOracle() -> Integer  {
        assert false;
        return 1;
    }
}

