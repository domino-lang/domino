composition MAC {
        const n: Integer;
        const b: Bool;
        const mac: fn Bits(n), Bits(n), Integer -> Bits(n);

    instance MAC = MAC {
        params {
        n: n,
        b: b,
        mac: mac,
        }
    }

    compose {
        adversary: {
            Init: MAC,
            Mac: MAC,
			Verify: MAC,
        },
    }
}
