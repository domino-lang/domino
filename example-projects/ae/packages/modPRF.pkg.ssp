package modPRF {
    params {
        n: Integer,
        prf: fn Bits(n), Bits(n)  -> Bits(n),
    }
    
    state {
        ltk: Maybe(Bits(n)),
    }
    
    import oracles {
        Set(k: Bits(n), h__: Bits(n)), 
    }
    
    oracle Eval(h: Integer, h_: Bits(n)) {
        if (ltk == None as Bits(n)) {
            ltk_ <-$ Bits(n);
	    ltk <- Some(ltk_);
        }

	ltk_ <- Unwrap(ltk);
        
        k <- prf(ltk_, h_);
        _ <- invoke Set(k, h_);
    } 
}
