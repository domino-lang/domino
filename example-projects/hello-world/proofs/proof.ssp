                invariant: [
                    ./proofs/invariant.smt2
                ]

                lemmas {
                    equal-aborts-Oracle: []
                    invariant:           [no-abort-Oracle]
                    same-output-Oracle:  [no-abort-Oracle]
                }
            }
        }

    }
}
