(define-lemma <relation-not-in-first-second-H7_1_1_0-H7_1_1_1-Send3>
    ((H710-old <GameState_H7_<$<!n!>$>>)
     (H711-old <GameState_H7_<$<!n!>$>>)
     (H710-return <OracleReturn_H7_<$<!n!>$>_KX_noprfkey_<$<!n!>$>_Send3>)
     (H711-return <OracleReturn_H7_<$<!n!>$>_KX_noprfkey_<$<!n!>$>_Send3>)
     (ctr Int) (msg (Tuple2 Bits_n Bits_n)))
  (let ((state (select H710-return.state.KX.State ctr)))
    (=> (and (not (is-mk-none state))
             (= (mk-some true) (select H710-return.state.KX.Fresh ctr)))
        (let ((sid  (el11-10 (maybe-get state))))
          (=> (not (is-mk-none sid))
              (and (not (= (mk-some ctr) (select H710-old.KX.First (maybe-get sid))))
                   (not (= (mk-some ctr) (select H710-old.KX.Second (maybe-get sid))))))))))


