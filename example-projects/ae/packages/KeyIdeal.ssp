package KeyIdeal {
    params {
      n: Integer,
    }
    
    state {
       K: Table(Bits(n), Bits(n)),
    }

    oracle Set(k: Bits(n), h: Bits(n))  {
        if (K[h]==None as Bits(n)) { // None as Bits(n) hat type Maybe(Bits(n))
            k_ <-$ Bits(n);
            K[h] <- Some(k_);
        }
    }
    
    oracle Get(h_: Bits(n)) -> Bits(n) {
        return  Unwrap(K[h_]);
    }

}
