package Key_bottom {
    params {
        n: Integer,
    }

    state {
        T: Table((Integer,Bits(*)),Bits(n)),
    }

    oracle GET(h: (Integer,Bits(*))) -> Bits(n) {
        assert (T[h] != None as Bits(n));
        k <- Unwrap(T[h]);
        return k;
    }

    oracle SET(h: (Integer,Bits(*)), k: Bits(n)) -> (Integer,Bits(*)) {
        assert (T[h] == None as Bits(n));
        T[h] <- Some(k);
        return h;
    }
}