package Enc0 {
    params {
        n: Integer,
        m: Integer,
        p: Integer,
        encn: fn Bits(n),Bits(n),Bits(n) -> Bits(m),
        encm: fn Bits(n),Bits(m),Bits(n) -> Bits(p),
        zeron: Bits(n),
        zerom: Bits(m),
    }

    state {
    }

    oracle ENCN(j: Integer,d: Bool, mzero: Bits(n), mone: Bits(n)) -> Bits(m) {
        K <- invoke GETKEYS(j);
        r <-$ Bits(n);
        c <- encn(K[d],mzero,r);
        return c;
    }

    oracle ENCM(j: Integer,d: Bool, mzero: Bits(m), mone: Bits(m)) -> Bits(p) {
        K <- invoke GETKEYS(j);
        r <-$ Bits(n);
        c <- encm(K[d],mzero,r);
        return c;
    }

}