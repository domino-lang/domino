package EKeys {
    params {
        n: Integer,
    }

    state {
        T: Table(Integer,Table(Bool,Bits(n))),
    }

    oracle GETKEYS(h: Integer) -> Table(Bool,Bits(n)) {
        assert not (T[h]==None as Table(Bool,Bits(n)));
        Z <- Unwrap(T[h]);
        return Z;
    }

    oracle SETKEYS(h: Integer, Z: Table(Bool,Bits(n))) -> () {
        assert (T[h]==None as Table(Bool,Bits(n)));
        T[h] <- Some(Z);
        return ;
    }

}
