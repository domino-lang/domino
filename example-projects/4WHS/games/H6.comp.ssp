composition H6 {
        const n: Integer;
        const zeron: Bits(n);
        const prf: fn Bits(n), (Integer, Integer, Bits(n), Bits(n), Bool) ->  Bits(n);
        const mac: fn Bits(n), Bits(n), Integer -> Bits(n);
        const btest: Bool;
        const bleast: Bool;
        const bprf: Bool;
    
	instance PRF = PRF {
	    params {
            b: bprf,
		    prf: prf,
		    n: n,
		}
    }
	instance Nonces = Nonces {
		params {
        	n: n,
			b: true,
		}
	}
    instance Prot = ProtNoKeyOnlyKid {
        params {
        n: n,
        zeron: zeron,
        mac: mac,
        }
    }
    instance KX = KX_noprfkey {
        params {
        btest: btest,
        bleast: bleast,
        n: n,
        zeron: zeron,
        mac: mac,
        }
    }

    compose {
        adversary: {
            NewKey:     PRF,
            NewSession: KX,
            Send1:      KX,
            Send2:      KX,
            Send3:      KX,
            Send4:      KX,
            Send5:      KX,
            Reveal:     KX,
            Test:       KX,
			SameKey:	KX,
 			AtMost:		KX,
 			AtLeast:	KX,
       },

        KX: {
            Run1:      Prot,
            Run2:      Prot,
            Run3:      Prot,
            Run4:      Prot,
            Run5:      Prot,
			Hon:       PRF,
			Eval:      PRF,
        },

		Prot: {
			  Sample:	Nonces,
  			  Eval:     PRF,
		}
    }
}
