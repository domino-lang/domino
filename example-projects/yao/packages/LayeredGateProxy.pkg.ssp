package LayeredGateProxy {
    params {
        p: Integer,
        d: Integer,
    }

    import oracles {
        LGATEGBLG(
            i: Integer,
            l: Integer,
            r: Integer,
            op: Table((Bool, Bool), Bool),
            j: Integer
        ) -> Table(Bits(p), Bool),
    }

    oracle GBLG(
        i: Integer,
        l: Integer,
        r: Integer,
        op: Table((Bool, Bool), Bool),
        j: Integer
    ) -> Table(Bits(p), Bool) {
        assert (i > 0);
        assert (i <= d);
        C <- invoke LGATEGBLG(i, l, r, op, j);
        return C;
    }
}