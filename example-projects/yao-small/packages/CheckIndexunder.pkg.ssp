package CheckIndexunder {
    params {
        t: Integer,
        n: Integer,
    }

    import oracles {
        GETA(h: Integer) -> Bits(n),
        SETBIT(h: Integer,zz:Bool) -> (),
    }


    oracle GETA(h: Integer) -> Bits(n) {
        assert (h == t);
        k <- invoke GETA(h);
        return k; 
    }

    oracle SETBIT(h: Integer, zz:Bool) -> () {
        assert (h == t);
        _ <- invoke SETBIT(h,zz);
        return ;
    }

}