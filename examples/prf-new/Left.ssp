composition Left
 {
const n: Integer;
const f: fn Bits(n),Bits(*) -> Bits(n);
 
instance key_top =
        Key_top
          {
          n: n,
          }

instance prf_left =
        Prf_left
          {
          f: f,
          n: n,
          }

compose { 
     adversary:
        { SET:  key_top,
          EVAL: prf,
        },
     prf_left:
        { GET:  key_top,
        },
        }
 }
