WHITESPACE = _{ " " | "\r" | "\n" | "\t" }

model = { "(" ~ modelline*~ ")" }
modelline = {"(define-fun" ~ name ~ "()" ~ tipe ~ value ~ ")"}

name = @{ ( ASCII_ALPHA | "_" | "-" | "<" | ">" ) ~ ( ASCII_ALPHA | ASCII_DIGIT | "_" | "-" | "<" | ">" )* }
tipe = { "Int" | "Bool" }
value = { ASCII_DIGIT+ | "true" | "false" }