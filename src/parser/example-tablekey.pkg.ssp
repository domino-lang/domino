package TableKey {
    params {
        n: Integer,
    }

    state {
        k: Table(Integer,Bits(n)),
    }

    oracle Get(i: Integer) -> Bits(n) {
        if ( k[i] == None as Bits(n)) {
            k_new <-$ Bits(n);
            k[i] <- Some(k_new);
        }

        k_ret <- Unwrap(k[i]);
        return k_ret;
    }

    oracle Set(i: Integer, k_new: Bits(n)) {
        if (k[i] != None as Bits(n)) {
            abort;
        }

        k[i] <- Some(k_new);
    }
}
