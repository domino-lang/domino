composition Monprfreal {
    const n: Integer;
    const prf: fn Bits(n), Integer, Integer, Bits(n), Bits(n), Bool ->  Bits(n);
    
    instance prf = PRFReal {
        params {
            n:   n,
            prf: prf,
        }
    }

    compose {
        adversary: {
            Eval: prf,
            Get:  prf,
        },
    }
}
