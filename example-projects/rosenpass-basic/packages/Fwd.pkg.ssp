package Fwd {
    params {
        kem_ct_len: Integer,
        kem_pk_len: Integer,
    }

    import oracles {
        Encaps(pk: Bits(kem_pk_len)) -> Bits(kem_ct_len),
    }

    oracle Encaps(pk: Bits(kem_pk_len)) -> Bits(kem_ct_len)  {
        y  <- invoke Encaps(pk);
        return y;
    }
}

