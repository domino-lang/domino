package Prf {
    params {
        f: fn Bits(n),Bits(*) -> Bits(n),
        n: Integer,
    }

    import oracles {
        Get(h: Integer) -> Bits(n),
        Set(h: (Integer,Bits(*)), k: Bits(n)) -> Integer,
    }

    oracle Eval(h: Integer, m: Bits(*)) -> (Integer,Bits(*)) {
        h <- GET(h);
        y <- f(h,m);
        hh <- (h,m);
        _ <- SET(hh,y);
        return hh;
    }
}
