package Key-top {
    params {
        n: Integer,
    }

    state {
        T: Table(Integer,Maybe(Bits(n))),
    }

    oracle Get(h: Integer) -> Bits(n) {
        assert (T[h] != None as Bits(n));
        k <- Unwrap(T[h]);
        return k;
    }

    oracle Set(h: Integer, k: Bits(n)) -> Integer {
        assert (T[h] == None as Bits(n));
        T[h] <- Some(k)
        return h;
    }
}