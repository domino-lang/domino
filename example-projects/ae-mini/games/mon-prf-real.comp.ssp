composition Monprfreal {
    const n: Integer;
  /*  const prf: fn Bits(n), Bits(n) -> Bits(n);*/
    
    instance prf = PRFReal {
        params {
            n:   n,
        /*  prf: prf,*/
        }
    }

    instance red = ReductionPRF {
        params {
            n:   n,
        }
    }

    compose {
        adversary: {
            Eval: red,
            Get:  red,
        },
            red: {
            Eval: prf,
        },
    }
}
