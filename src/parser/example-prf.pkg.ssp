package PRF {
    params {
        n: Integer,
        f: fn Bits(n), Bits(n) -> Bits(n),
    }

    state {}
    
    import oracles {
        Get() -> Bits(n),
    }

    oracle Eval(msg: Bits(n)) -> Bits(n) {
        /* we have to put the <- directly after the k here. this might be a bug in pest.
           TODO: construct minimum working example for bug report */
        k <- Get(); 
        out <- f(k, msg);
        return out;
    }
}