package Fwd {
    params {
        n: Integer,
    }

    state {
        ctr: Integer,
    }

    import oracles {
        UsefulOracle() -> (Integer,Bits(n)),
    }

    oracle UsefulOracle() -> (Integer,Bits(n))  {
        y  <- invoke UsefulOracle();
        return y;
    }

    oracle UselessOracle() -> Integer  {
        assert false;
        return 1;
    }
}

