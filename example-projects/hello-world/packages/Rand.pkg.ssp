package Rand {
    params {
        n: Integer,
    }

    state {
          ctr: Integer,
    }

    oracle UsefulOracle() -> (Integer, Bits(n))  {
        ctr  <- (ctr + 1);
        rand <-$ Bits(n);
        return (ctr, rand);
    }

    oracle UselessOracle() -> Integer  {
        assert false;
        return 1;
    }
}

