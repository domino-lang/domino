package PRFReal {
    params {
        n: Integer,
     /* prf: fn Bits(n), Bits(n) -> Bits(n),*/
    }
    
    state {
        ltk: Maybe(Bits(n)),
    }

    oracle Eval(h: Bits(n)) -> Bits(n) {
        if (ltk == None as Bits(n)) {
            ltk_ <-$ Bits(n);
	          ltk <- Some(ltk_);
        }       

        return h; /* prf(Unwrap(ltk), h);*/
    } 
}
