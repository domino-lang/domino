package Prf {
    params {
        f: fn Bits(n),Bits(*) -> Bits(n),
        n: Integer,
    }

    import oracles {
        GET(h: Integer) -> Bits(n),
        SET(h: (Integer,Bits(*)), k: Bits(n)) -> (Integer,Bits(*)),
    }

    oracle EVAL(h: Integer, m: Bits(*)) -> (Integer,Bits(*)) {
        k <- GET(h);
        y <- f(k,m);
        hh <- (h,m);
        _ <- SET(hh,y);
        return hh;
    }
}
