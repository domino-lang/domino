package PRFIdeal {
	params {
		n: Integer,
	}
    
    state {
          K: Table(Integer,Bits(n))
          }
        
    oracle Eval(h: Integer, h: Bits(n)) {
        if (K[h] == None as Bits(n)) {
           y    <-$ Bits(n);
           K[h] <- Some(y);
        }    
        return Unwrap(K[h]);
    } 
}

