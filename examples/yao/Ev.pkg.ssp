package Ev {
    params {
        n: Integer,
    }

    state {
    }

    oracle EVAL(j: Integer,l: Integer,r: Integer,op: fn (Bool,Bool) -> Bool) -> Bits(m) {
        zl <- invoke GETBIT(l);
        zr <- invoke GETBIT(r);
        z <- op(zl,zr);
        _ <- invoke SETBIT(j,z);
        return ;
    }

}